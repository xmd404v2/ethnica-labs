# Environment variables for Ethnica Labs
# Copy this file to .env.local and fill in your values

# Database connection
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/ethnica?schema=public"

# Auth
NEXTAUTH_SECRET="your-secret-key-here-change-for-production"
NEXTAUTH_URL="http://localhost:3000"

# Mapbox - Required for map functionality
# Get your token at https://account.mapbox.com/
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token-here

# Privy Authentication
# Get your Privy App ID at https://console.privy.io/
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-app-id-here 